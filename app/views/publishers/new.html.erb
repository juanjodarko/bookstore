<h2>
	Agregar Nueva Editorial
</h2>

<%=form_for @publisher do |f|%>
<div class="form-group">
	<%= f.label :name %><br>
	<%= f.text_field :name, class: 'form_control' %><br>
</div>

<%= f.submit 'Agregar', class:'btn btn-primary' %>
<%= link_to 'Cancelar', authors_path, class:'btn btn-default' %>
<%end%>
